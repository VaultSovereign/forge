{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "rootDir": ".",
    "outDir": "dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "cli/**/*",
    "dispatcher/**/*",
    "types/**/*",
    "agent/**/*",
    "tools/**/*",
    "mcp/**/*",
    "scripts/**/*",
    "reality_ledger/**/*",
    "providers/**/*"
  ]
}
